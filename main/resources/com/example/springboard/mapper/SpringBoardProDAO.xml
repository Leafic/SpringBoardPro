<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.example.springboard.mapper.SpringBoardProDAO">

    <select id="selectBoardID" parameterType="String" resultType="com.example.springboard.vo.SpringBoardProVO">
        SELECT *
        FROM tb_board
        WHERE id = #{id}
        order by id desc;
    </select>

</mapper>
